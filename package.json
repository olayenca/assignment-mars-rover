{
  "name": "assignment-mars-rover",
  "version": "1.0.0",
  "description": "Nandos take home assignment - Mars Rover",
  "main": "index.ts",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "server": "npx ts-node src/server/server.ts",
    "rover": "npx ts-node src/index.ts",
    "start": "pnpm run server & pnpm run rover",
    "test": "npx vitest --no-watch",
    "audit": "audit-ci --config ./audit-ci.json5"
  },
  "keywords": [
    "nandos",
    "mars",
    "rover"
  ],
  "author": "Olayinka Otuniyi",
